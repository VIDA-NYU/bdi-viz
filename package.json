{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "redis-server": "redis-server --port 6380",
    "celery-dev": "celery -A api.index.celery worker --pool threads --loglevel=debug",
    "flask-dev": "FLASK_DEBUG=1 python3 -m flask --app api/index run -p 5328 --debug",
    "next-dev": "next dev",
    "celery-start": "celery -A api.index.celery worker --pool threads --loglevel=info",
    "next-start": "next start",
    "flask-start": "python3 -m flask --app api/index run -p 5328",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\" \"pnpm run redis-server\" \"pnpm run celery-dev\"",
    "start": "concurrently \"pnpm run next-start\" \"pnpm run flask-start\" \"pnpm run redis-server\" \"pnpm run celery-start\"",
    "build": "pip3 install -r requirements.txt --break-system-packages && next build",
    "lint": "next lint"
  },
  "dependencies": {
    "@mui/icons-material": "^6.1.10",
    "@mui/lab": "^6.0.0-beta.25",
    "@mui/material": "^6.1.10",
    "@mui/x-data-grid": "^7.25.0",
    "@observablehq/plot": "^0.6.16",
    "@uiw/react-split": "^5.9.3",
    "axios": "^1.7.9",
    "concurrently": "^8.0.1",
    "d3": "^7.9.0",
    "material-react-table": "^3.1.0",
    "next": "^14.2.21",
    "pnpm": "^9.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.5",
    "react-toastify": "^10.0.6",
    "recharts": "^2.15.1",
    "uuid": "^11.1.0"
  },
  "packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab",
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
