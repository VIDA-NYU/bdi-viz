{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "kill-redis-6380": "bash -c 'if command -v lsof >/dev/null 2>&1; then PIDS=$(lsof -ti :6380 || true); if [ -n \"$PIDS\" ]; then kill -9 $PIDS || true; fi; else pkill -f \"redis-server.*6380\" || true; fi'",
    "redis-server": "mkdir -p .cache/redis && redis-server --port 6380 --dir .cache/redis --save '' --appendonly no",
    "celery-dev": "celery -A api.index.celery worker --pool threads -Q matching,new_matcher,ontology -Ofair --loglevel=debug --logfile=celery.log",
    "flask-dev": "FLASK_DEBUG=1 python3 -m flask --app api/index run -p 5328 --debug",
    "next-dev": "next dev",
    "celery-start": "celery -A api.index.celery worker -n match@%h --pool threads -Q matching,new_matcher -Ofair --loglevel=info --logfile=celery.log",
    "celery-ontology": "celery -A api.index.celery worker -n onto@%h --pool prefork -Q ontology -c 1 -Ofair --loglevel=info --logfile=celery.log",
    "next-start": "next start",
    "flask-start": "python3 -m flask --app api/index run -p 5328",
    "predev": "pnpm run kill-redis-6380",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\" \"pnpm run redis-server\" \"pnpm run celery-dev\"",
    "prestart": "pnpm run kill-redis-6380",
    "start": "concurrently \"pnpm run next-start\" \"pnpm run flask-start\" \"pnpm run redis-server\" \"pnpm run celery-start\" \"pnpm run celery-ontology\"",
    "build": "pip3 install -U pip && pip3 install -r requirements.txt --break-system-packages && next build",
    "lint": "next lint",
    "test": "jest"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^6.1.10",
    "@mui/lab": "^6.0.0-beta.25",
    "@mui/material": "^6.1.10",
    "@mui/x-data-grid": "^7.25.0",
    "@observablehq/plot": "^0.6.16",
    "@uiw/react-split": "^5.9.3",
    "axios": "^1.7.9",
    "concurrently": "^8.0.1",
    "d3": "^7.9.0",
    "material-react-table": "^3.1.0",
    "next": "^14.2.21",
    "pnpm": "^9.15.0",
    "postcss": "^8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.5",
    "react-toastify": "^10.0.6",
    "recharts": "^2.15.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "uuid": "^11.1.0"
  },
  "packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/d3": "^7.4.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18",
    "babel-jest": "^30.0.4",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "ts-jest": "^29.4.0"
  }
}
